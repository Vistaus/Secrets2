#!/bin/bash
# Main executable script for Secrets Password Manager
# This file is generated by meson from secrets.in

PYTHON="/usr/bin/python"
INSTALLED_PYTHONPATH="/app/lib/python3.11/site-packages/"
PROJECT_ID="io.github.tobagin.secrets"

# Set up Python path for installed version
export PYTHONPATH="$INSTALLED_PYTHONPATH:$PYTHONPATH"

# Run the application
exec "$PYTHON" -m secrets.main "$@"
